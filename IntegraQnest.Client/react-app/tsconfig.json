{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
	"rootDir": "./src",
    "outDir": "../wwwroot/js",
    "strict": true,
	  "esModuleInterop": true,
	  "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*"],
  "exclude": ["../wwwroot/js", "node_modules"]
}